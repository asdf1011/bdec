<protocol>
    <sequence name="a">
        <field name="b" length="8" />
        <field name="c" type="integer" length="8" if="${b} == 0" />
        <field name="d" type="integer" length="8" if="${b} != 5" />
        <field name="e" type="integer" length="8" if="${b} > 5" />
        <field name="f" type="integer" length="8" if="${b} >= 7" />
    </sequence>
</protocol>
